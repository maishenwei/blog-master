<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.mapper.PostMapper">
	<insert id="insert">
		insert into blog.blog_post(id,title,tagname,image,lunbo,introduce,content,createtime,lastBlog,lastBlogName,nextBlog,nextBlogName,userid)
		values(#{id},#{title},#{tagname},#{image},#{lunbo},#{introduce},#{content},#{createTime},#{lastBlog},#{lastBlogName},#{nextBlog},#{nextBlogName},#{userid})
	</insert>

</mapper>
